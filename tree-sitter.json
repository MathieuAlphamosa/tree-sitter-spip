{
  "grammars": [
    {
      "name": "spip",
      "camelcase": "Spip",
      "scope": "source.spip",
      "path": ".",
      "file-types": [
        "html"
      ],
      "injection-regex": "spip",
      "first-line-regex": "<BOUCLE_|\\(#[A-Z]|<INCLURE\\{"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the SPIP template language",
    "authors": [
      {
        "name": "Mathieu Alphamosa"
      }
    ],
    "links": {
      "repository": "https://github.com/MathieuAlphamosa/tree-sitter-spip"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
